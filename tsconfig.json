{
    "compilerOptions": {
      "target": "ES2018",
      "module": "ESNext",
      "moduleResolution": "Node",
      "lib": [
        "ESNext",
        "ESNext.AsyncIterable",
        "DOM"
      ],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "strictPropertyInitialization": false,
      "allowJs": true,
      "sourceMap": true,
      "declaration": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "@types/node",
        "@nuxt/types",
        "@nuxt/vue-app"
      ],
      "typeRoots": [
        "./types"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }